{
  "realm": "healthcare",
  "enabled": true,
  "clients": [
    {
      "clientId": "HealthApp",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3010/*",
        "http://localhost:3011/*",
        "http://localhost:3012/*"
      ],
      "webOrigins": [
        "http://localhost:3010",
        "http://localhost:3011",
        "http://localhost:3012"
      ],
      "protocol": "openid-connect",
      "fullScopeAllowed": true,
      "directAccessGrantsEnabled": true,
      "protocolMappers": [
        {
          "name": "realm-roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "realm_access.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "access.token.claim": "true",
            "id.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        },
        {
          "name": "client-roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "claim.name": "resource_access.${client_id}.roles",
            "jsonType.label": "String",
            "multivalued": "true",
            "access.token.claim": "true",
            "id.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    }
  ],
  "roles": {
    "realm": [
      {"name": 0},{"name": 1},{"name": 2},{"name": 3},{"name": 4},
      {"name": 5},{"name": 6},{"name": 7},{"name": 8},{"name": 9},
      {"name": 10},{"name": 11},{"name": 12},{"name": 13},{"name": 14},
      {"name": 15},{"name":16},{"name": 17},{"name": 18}
    ]
  },
  "users": [
    {
      "username": "test",
      "enabled": true,
      "credentials": [{"type": "password","value":"test"}],
      "realmRoles": [0]
    }
  ]
}